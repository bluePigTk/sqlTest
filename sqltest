1.
SELECT b.STUDENT_NO 学号,b.STUDENT_NAME 姓名 FROM HAND_STUDENT_CORE a,HAND_STUDENT b WHERE a.STUDENT_NO=b.STUDENT_NO and a.COURSE_NO NOT IN(SELECT COURSE_NO FROM HAND_COURSE WHERE TEACHER_NO=(select TEACHER_NO from HAND_TEACHER WHERE Teacher_NAME='谌燕'))
2.
SELECT B.STUDENT_NO 学号,B.STUDENT_NAME 姓名 FROM	(SELECT STUDENT_NO ,count(*) TOTAL FROM HAND_STUDENT_CORE  GROUP BY STUDENT_NO) A,HAND_STUDENT B WHERE A.STUDENT_NO=B.STUDENT_NO AND A.TOTAL<(SELECT COUNT(*) FROM HAND_COURSE )
3.
SELECT C.STUDENT_NO 学号,C.STUDENT_NAME 姓名 FROM  (SELECT * FROM HAND_STUDENT_CORE WHERE COURSE_NO='c001') A ,(SELECT * FROM HAND_STUDENT_CORE WHERE COURSE_NO='c002') B,HAND_STUDENT C WHERE A.STUDENT_NO=B.STUDENT_NO AND A.CORE>B.CORE AND A.STUDENT_NO=C.STUDENT_NO
4.
SELECT COURSE_NO,AVG(CORE) AVG FROM HAND_STUDENT_CORE  GROUP BY COURSE_NO  ORDER BY AVG DESC

SELECT a.COURSE_NO, b.jige/a.total*100 及格率 FROM (SELECT COURSE_NO,count(*) total from HAND_STUDENT_CORE GROUP BY COURSE_NO) a,
(select COURSE_NO, count(*) jige from  HAND_STUDENT_CORE where core >=60  GROUP BY COURSE_NO)b WHERE a.COURSE_NO=b.COURSE_NO DESC
5.
