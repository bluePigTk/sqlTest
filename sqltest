7.
8.
 SELECT C.STUDENT_NO,C.STUDENT_NAME,B.COURSE_NO,B.CORE  FROM
(SELECT course_no,max(core) max FROM HAND_STUDENT_CORE WHERE COURSE_NO IN(SELECT COURSE_NO FROM HAND_COURSE WHERE TEACHER_NO=(select TEACHER_NO from HAND_TEACHER WHERE Teacher_NAME='谌燕')) GROUP BY COURSE_NO)A,HAND_STUDENT_CORE B,HAND_STUDENT C WHERE A.COURSE_NO=B.COURSE_NO AND A.MAX=B.CORE AND C.STUDENT_NO=B.STUDENT_NO
9.
SELECT C.STUDENT_NO,C.STUDENT_NAME,B.AVG
FROM
(SELECT A.STUDENT_NO,A.AVG AVG FROM
(SELECT STUDENT_NO,COUNT(*) TOTAL,AVG(CORE) AVG  from  HAND_STUDENT_CORE WHERE CORE<60 GROUP BY STUDENT_NO) A WHERE TOTAL>=2)B,HAND_STUDENT C WHERE
B.STUDENT_NO=C.STUDENT_NO
